{
  "name": "phosphor-svelte",
  "version": "1.0.0",
  "description": "A clean and friendly icon family for Svelte",
  "license": "MIT",
  "type": "module",
  "svelte": "./dist/lib/index.js",
  "types": "./dist/lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/lib/index.d.ts",
      "import": "./dist/lib/index.js",
      "svelte": "./dist/lib/index.js"
    },
    "./lib": {
      "types": "./dist/lib/index.d.ts",
      "import": "./dist/lib/index.js",
      "svelte": "./dist/lib/index.js"
    },
    "./lib/*": {
      "types": "./dist/lib/*.svelte.d.ts",
      "svelte": "./dist/lib/*.svelte"
    },
    "./vite": {
      "types": "./dist/vite/index.d.ts",
      "import": "./dist/vite/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [ "dist" ],
  "scripts": {
    "build": "tsc && node scripts/build.js",
    "postinstall": "npm run build"
  },
  "peerDependencies": {
    "svelte": "^5.0.0",
    "vite": ">=6"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  },
  "dependencies": {
    "zimmerframe": "1.1.4",
    "magic-string": "0.30.13"
  },
  "devDependencies": {
    "@types/node": "22.10.2",
    "@types/estree": "1.0.8",
    "@types/fs-extra": "11.0.4",
    "@sveltejs/vite-plugin-svelte": "6.2.1",
    "rollup": "4.52.4",
    "fs-extra": "11.3.2",
    "html-minifier-terser": "7.2.0",
    "jsdom": "27.0.0",
    "memfs": "4.49.0",
    "svelte": "5.39.11",
    "typescript": "5.9.3",
    "vitest": "3.2.4",
    "log-update": "7.0.1",
    "p-map": "7.0.3"
  }
}
